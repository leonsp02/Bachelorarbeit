      "tokens": [
        "Every",
        "weekday",
        "morning",
        ",",
        "the",
        "database",
        "is",
        "backed",
        "up",
        "and",
        "then",
        "it",
        "is",
        "checked",
        "to",
        "see",
        "whether",
        "the",
        "Account",
        "Defaulter",
        "table",
        "has",
        "new",
        "records",
        ".",
        "If",
        "no",
        "new",
        "records",
        "are",
        "found",
        ",",
        "then",
        "the",
        "process",
        "should",
        "check",
        "the",
        "CRM",
        "system",
        "to",
        "see",
        "whether",
        "new",
        "returns",
        "have",
        "been",
        "filed",
        ".",
        "If",
        "new",
        "returns",
        "exist",
        ",",
        "then",
        "register",
        "all",
        "defaulting",
        "accounts",
        "and",
        "customers",
        ".",
        "If",
        "the",
        "defaulting",
        "client",
        "codes",
        "have",
        "not",
        "been",
        "previously",
        "advised",
        ",",
        "produce",
        "another",
        "table",
        "of",
        "defaulting",
        "accounts",
        "and",
        "send",
        "to",
        "account",
        "management",
        ".",
