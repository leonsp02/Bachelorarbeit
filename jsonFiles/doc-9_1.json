        "of",
        "this",
        "must",
        "be",
        "completed",
        "by",
        "2",
        ":",
        "30",
        "pm",
        ",",
        "if",
        "it",
        "is",
        "not",
        ",",
        "then",
        "an",
        "alert",
        "should",
        "be",
        "sent",
        "to",
        "the",
        "supervisor",
        ".",
        "Once",
        "the",
        "new",
        "defaulting",
        "account",
        "report",
        "has",
        "been",
        "completed",
        ",",
        "check",
        "the",
        "CRM",
        "system",
        "to",
        "see",
        "whether",
        "new",
        "returns",
        "have",
        "been",
        "filed",
        ".",
        "If",
        "new",
        "returns",
        "have",
        "been",
        "filed",
        ",",
        "reconcile",
